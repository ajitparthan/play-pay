<div class="modal-header">
	<h3>PlayPay Users</h3>
</div>

<div class="modal-body">
	<div class="well">
		<table class="table">
			<thead>
				<tr>
					<th>Name</th>
					<th>Email</th>
					<th>Balance</th>
					<th>Created</th>
					<th>Last Updated</th>
			</thead>
			<tbody>
				{{#each user in model}}
					<tr>
						<td>{{user.name}}</td>
						<td>{{user.email_id}}</td>
						<td>{{user.balance}}</td>
						<td>{{user.created}}</td>
						<td>{{user.last_updated}}</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
